<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="ArubaSignServicePortBinding" messageEncoding="Mtom">
                    <security mode="Transport" />
                </binding>
                <binding name="ArubaSignServicePortBinding1" messageEncoding="Mtom" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="https://arss.demo.firma-automatica.it:443/ArubaSignService/ArubaSignService" binding="basicHttpBinding" bindingConfiguration="ArubaSignServicePortBinding" contract="Aruba.ArubaSignService" name="ArubaSignServicePort" />
        </client>
    </system.serviceModel>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <appSettings>
    <add key="proxySignUrl" value="https://testdev.proxysign.it" />
  </appSettings>
</configuration>