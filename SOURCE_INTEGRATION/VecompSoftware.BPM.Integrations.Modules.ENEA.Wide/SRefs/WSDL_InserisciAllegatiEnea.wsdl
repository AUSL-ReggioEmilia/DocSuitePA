	targetNamespace="http://Wide/WebService.nsf/InserisciAllegatiService.wsdl">
	<wsdl:types>
		<xsd:schema targetNamespace="http://Wide/WebService.nsf/InserisciAllegatiService.wsdl"
			xmlns:tns="http://Wide/WebService.nsf/InserisciAllegatiService.wsdl"
			xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:element name="inserisciAllegatiRequest">
		  <xsd:complexType>
		 <xsd:sequence>
                 <xsd:element name="NumeroProtocollo" type="tns:Obbl_Type"/>
                 <xsd:element name="TipoAllegato" type="tns:TipoAllegato_Type"/>
                 <xsd:element name="Tipologia" type="tns:Tipologia_Type" maxOccurs="unbounded"/>
                 <xsd:element name="Titolo" type="tns:Obbl_Type256" maxOccurs="unbounded"/>
                 <xsd:element name="Descrizione" type="tns:Obbl_Type256" maxOccurs="unbounded"/>	
                 <xsd:element name="PathFile" type="tns:Obbl_Type256"/>   
		     <xsd:element name="NomeFile" type="tns:Obbl_Type256" maxOccurs="unbounded"/>  
