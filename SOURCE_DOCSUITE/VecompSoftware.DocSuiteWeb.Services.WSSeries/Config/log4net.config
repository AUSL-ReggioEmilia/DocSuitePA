<?xml version="1.0" encoding="utf-8" ?>
<log4net>
  <appender name="WSSeriesLog" type="log4net.Appender.RollingFileAppender">
    <file value="logs/WSSeriesLog.log"/>
    <appendToFile value="true"/>
    <rollingStyle value="Size"/>
    <maxSizeRollBackups value="10"/>
    <maximumFileSize value="100KB"/>
    <staticLogFileName value="true"/>
    <filter type="log4net.Filter.LevelRangeFilter">
      <levelMin value="WARN" />
      <levelMax value="FATAL" />
    </filter>
    <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%d{dd/MM/yyyy HH:mm:ss.fff} [%t] %-5p %c - %m %newlineThreadUser: %username %newlinePostazione: %property{log4net:HostName} %newline"/>
    </layout>
  </appender>
  
  <appender name="SqlLog" type="log4net.Appender.RollingFileAppender">
    <file value="logs/sql.txt"/>
    <appendToFile value="true"/>
    <rollingStyle value="Size"/>
    <maxSizeRollBackups value="10"/>
    <maximumFileSize value="100KB"/>
    <staticLogFileName value="true"/>
    <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%d{HH:mm:ss.fff} [%t] %-5p %username %property{log4net:HostName} %c - %m%n"/>
    </layout>
  </appender>
  <root>
    <level value="ALL"/>
  </root>
  <logger name="WSSeriesLog" additivity="false">
    <level value="ALL"/>
    <appender-ref ref="WSSeriesLog"/>
  </logger>
  <!--<logger name="NHibernate.SQL" additivity="false">
    <level value="DEBUG"/>
    <appender-ref ref="SqlLog"/>
  </logger>
  <logger name="NHibernate.Loader.Loader" additivity="false">
    <level value="INFO"/>
    <appender-ref ref="SqlLog"/>
  </logger>
  <logger name="NHibernate" additivity="false">
    <level value="WARN"/>
    <appender-ref ref="SqlLog"/>
  </logger>-->
  
</log4net>