<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" schema="dbo" assembly="VecompSoftware.DocSuiteWeb.Data.Entity" namespace="VecompSoftware.DocSuiteWeb.Data.Entity.Workflows">

  <class name="WorkflowActivityLog" table="WorkflowActivityLogs" lazy="true" >
    <id name="Id" column="IdWorkflowActivityLog" >
      <generator class="guid.comb" />
    </id>
    <version name="Timestamp" generated="always" unsaved-value="null" type="BinaryBlob">
      <column name="Timestamp" not-null="true" sql-type="timestamp" />
    </version>
    <many-to-one name="WorkflowActivity">
      <column name="IdWorkflowActivity" not-null="true" />
    </many-to-one>
    <property name="LogDate">
      <column name="LogDate" not-null="true" />
    </property>
    <property name="SystemComputer">
      <column name="SystemComputer" not-null="true" />
    </property>
    <property name="SystemUser">
      <column name="SystemUser" not-null="true" />
    </property>
    <property name="LogType">
      <column name="LogType" not-null="true" />
    </property>
    <property name="LogDescription">
      <column name="LogDescription" not-null="true" />
    </property>
    <property name="Severity">
      <column name="Severity" not-null="false" />
    </property>
    <property name="RegistrationDate">
      <column name="RegistrationDate" not-null="true" />
    </property>
  </class>

</hibernate-mapping>