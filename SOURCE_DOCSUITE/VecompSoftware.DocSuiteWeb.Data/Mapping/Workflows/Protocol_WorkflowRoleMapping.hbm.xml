<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" schema="dbo" assembly="VecompSoftware.DocSuiteWeb.Data.Entity" namespace="VecompSoftware.DocSuiteWeb.Data.Entity.Workflows">

  <class name="WorkflowRoleMapping" table="WorkflowRoleMappings" lazy="true" >
    <id name="Id" column="IdWorkflowRoleMapping" >
      <generator class="guid.comb" />
    </id>
    
    <version name="Timestamp" generated="always" unsaved-value="null" type="BinaryBlob">
      <column name="Timestamp" not-null="true" sql-type="timestamp" />
    </version> 
    
    <property name="MappingTag">
      <column name="MappingTag" not-null="true" />
    </property>  
    <property name="AuthorizationType">
      <column name="AuthorizationType" not-null="true" />
    </property>
    <property name="IdInternalActivity">
      <column name="IdInternalActivity" not-null="false" />
    </property>
    <property name="AccountName">
      <column name="AccountName" not-null="false" />
    </property>
    <property name="RegistrationUser">
      <column name="RegistrationUser" not-null="true" />
    </property>
    <property name="RegistrationDate">
      <column name="RegistrationDate" not-null="true" />
    </property>
    <property name="LastChangedDate">
      <column name="LastChangedDate" not-null="false" />
    </property>
    <property name="LastChangedUser">
      <column name="LastChangedUser" not-null="false" />
    </property>    
    
    <many-to-one name="WorkflowRepository" column="IdWorkflowRepository" class ="WorkflowRepository" not-null="false" />     
    <many-to-one name="Role" column="IdRole" class="VecompSoftware.DocSuiteWeb.Data.Role" not-null="false" />
  </class>

</hibernate-mapping>