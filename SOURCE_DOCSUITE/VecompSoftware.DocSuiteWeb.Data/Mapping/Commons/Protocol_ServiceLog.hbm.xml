<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" schema="dbo" assembly="VecompSoftware.DocSuiteWeb.Data" namespace="VecompSoftware.DocSuiteWeb.Data">

  <class name="ServiceLog" table="ServiceLog">
    
    <id name="Id" column="Id">
      <generator class="assigned" />
    </id>
    
    <property name="DateTime" column="DateTime" />
    <property name="Server" column="Server" type="AnsiString" />
    <property name="Client" column="Client" type="AnsiString" />
    <property name="Application" column="Application" type="AnsiString" />
    <property name="Session" column="Session" type="AnsiString" />
    <property name="Level" column="Level" />
    <property name="Text" column="Text" type="AnsiString" />
    
  </class>

</hibernate-mapping>