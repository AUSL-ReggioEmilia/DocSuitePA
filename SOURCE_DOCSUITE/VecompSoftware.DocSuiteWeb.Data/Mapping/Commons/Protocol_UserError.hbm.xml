  <class name="UserError" table="UserError">
    
    <id name="Id" column="Id">
      <generator class="assigned" />
    </id>
    
    <property name="SystemUser" column="SystemUser" type="AnsiString" />
    <property name="SystemServer" column="SystemServer" type="AnsiString" />
    <property name="SystemComputer" column="SystemComputer" type="AnsiString" />
    <property name="ModuleName" column="ModuleName" type="AnsiString" />
    <property name="ErroreLevel" column="ErroreLevel" type="AnsiChar" />
    <property name="ErrorDate" column="ErrorDate" />
    <property name="ErrorDescription" column="ErrorDescription" type="AnsiString" />
    
  </class>
  
</hibernate-mapping>