<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" schema="dbo" assembly="VecompSoftware.DocSuiteWeb.Data" namespace="VecompSoftware.DocSuiteWeb.Data">

  <class name="OChartItemWorkflow" table="OChartItemWorkflows">

    <id name="Id" column="IdOChartItemWorkflow" >
      <generator class="guid.comb" />
    </id>
    <property name="LastChangedDate">
      <column name="LastChangedDate" not-null="false" />
    </property>
    <property name="LastChangedUser">
      <column name="LastChangedUser" not-null="false" />
    </property>
    <property name="RegistrationUser">
      <column name="RegistrationUser" not-null="true" />
    </property>
    <property name="RegistrationDate">
      <column name="RegistrationDate" not-null="true" />
    </property>
    <property name="IdWorkflowRepository">
      <column name="IdWorkflowRepository" not-null="true" />
    </property>
    <many-to-one name="OChartItem">
      <column name="IdOChartItem" not-null="true" />
    </many-to-one>
    <bag name="RoleUsers" cascade="save-update">
      <key column="IdOChartItemWorkflow" />
      <one-to-many class="RoleUser" />
    </bag>
  </class>

</hibernate-mapping>
